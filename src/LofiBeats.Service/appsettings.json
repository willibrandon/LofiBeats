{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Telemetry": {
    "EnableLocalFile": true,
    "EnableSeq": true,
    "SeqServerUrl": "http://localhost:5341",
    "SeqApiKey": null,
    "MaxBufferSize": 100,
    "FlushIntervalMinutes": 1,
    "MaxFileSizeMB": 10,
    "MaxFileAgeDays": 30
  }
}
