{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Telemetry": {
    "IsEnabled": false,
    "EnableLocalFile": false,
    "EnableSeq": false,
    "MaxBufferSize": 100,
    "FlushIntervalMinutes": 1,
    "MaxFileSizeMB": 10,
    "MaxFileAgeDays": 30
  }
}
