{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Telemetry": {
    "IsEnabled": false,
    "EnableLocalFile": true,
    "EnableSeq": false,
    "SeqServerUrl": "http://localhost:5341",
    "MaxBufferSize": 100,
    "FlushIntervalMinutes": 1,
    "MaxFileSizeMB": 10,
    "MaxFileAgeDays": 30
  },
  "WebSocket": {
    "EndpointPath": "/ws/lofi",
    "KeepAliveIntervalSeconds": 30,
    "MaxMessageSize": 32768,
    "RequireAuthentication": false,
    "MaxConcurrentConnections": 100,
    "MessageRateLimit": 10
  }
}
