{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "LofiBeats.Service.WebSocket": "Debug"
    }
  },
  "Telemetry": {
    "EnableLocalFile": true,
    "EnableSeq": true,
    "SeqServerUrl": "http://localhost:5341",
    "IsTestEnvironment": true
  },
  "WebSocket": {
    "RequireAuthentication": false,
    "MaxConcurrentConnections": 100,
    "MessageRateLimit": 20
  }
}
